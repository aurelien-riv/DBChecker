{
    "name": "DBChecker",
    "license": "GPL-3.0-only",
    "type": "project",
    "description": "Performs automated health checks on your database",
    "require": {
        "php": "^7.1",
        "symfony/config": "^4.0",
        "symfony/yaml":   "^4.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/cache": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "files": [ "src/DBCheckerConstants.php" ],
        "psr-4": {
            "DBChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBCheckerTests\\": "tests"
        }
    }
}
